{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["prettier", "airbnb"],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "no-unused-vars": "off",
    "jsx-a11y/media-has-caption": "off",
    "react/react-in-jsx-scope": "off",
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "react/prop-types":"off",
    "react/jsx-props-no-spreading": "off"
  }
}
